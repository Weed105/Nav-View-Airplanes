<Window x:Class="Nav_View_Airplanes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nav_View_Airplanes"
        mc:Ignorable="d"
        DataContext="{Binding mWindowViewModel, Source={StaticResource ViewModelLocator}}"
        WindowState="Maximized"
        Title="Nav View Airplanes" Height="450" Width="800">
    <Grid>
        <Frame Content="{Binding PageSource}" NavigationUIVisibility="Hidden" ></Frame>
    </Grid>
</Window>
